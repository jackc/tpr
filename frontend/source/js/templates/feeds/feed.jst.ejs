<div class="name"><a href="<%= url %>"><%= name %></a></div>
<div class="meta">
  <% if(last_publication_time) { %>
    Last published
    <time datetime="<%= last_publication_time.toISOString() %>"><%= last_publication_time.toTPRString() %></time>
  <% } %>
</div>
<% if(failure_count > 0) { %>
  <div class="error">
    <%= last_failure %>
  </div>
<% } %>
<div class="actions">
  <a class="unsubscribe" href="#">Unsubscribe</a>
</div>
